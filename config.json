{
  "google_sheet": {
    "sheet_id": "1KIC5h0oCx44kuYBIMa451csC4BW56mTXJQOgFLbgpzU",
    "sheet_name": "Daily Ops",
    "date_column": "Date"
  },
  "auth_method": "oauth",
  "credentials_file": "credentials.json",
  "oauth_credentials_file": "oauth_credentials.json",
  "oauth_token_file": "token.pickle",
  "csv_folder": "daily_data",
  "overwrite_behavior": "ask",
  "csv_mappings": {
    "Sales category summary.csv": {
      "column_mappings": {
        "Net sales": "Net Sales ($)",
        "Gross sales": "Gross Sales ($)",
        "Tax amount": "Sales Tax ($)"
      },
      "rules": {
        "Net sales": {
          "type": "first"
        }
      },
      "special_mappings": {
        "category_breakdown": {
          "type": "category_breakdown",
          "category_column": "Sales category",
          "value_column": "Net sales",
          "target_columns": {
            "Food": "Food Sales ($)",
            "Liquor": "Alcohol Sales ($)",
            "Beer": "Alcohol Sales ($)"
          }
        }
      }
    },
    "Net sales summary.csv": {
      "column_mappings": {
        "Net sales": "Net Sales ($)",
        "Gross sales": "Gross Sales ($)"
      }
    },
    "Payments summary.csv": {
      "column_mappings": {
        "Amount": "Payments Total ($)",
        "Tips": "Tips",
        "Total": "Total Collected ($)"
      },
      "rules": {
        "Amount": {
          "type": "total"
        }
      }
    },
    "Revenue summary.csv": {
      "column_mappings": {
        "Net sales": "Net Sales ($)",
        "Gratuity": "Gratuity",
        "Tax amount": "Sales Tax ($)",
        "Tips": "Tips",
        "Total": "Total Collected ($)"
      }
    },
    "Tip summary.csv": {
      "column_mappings": {
        "Total tips": "Tips",
        "Tips collected": "Tips Collected"
      }
    },
    "Tax summary.csv": {
      "column_mappings": {
        "Tax amount": "Sales Tax ($)"
      },
      "rules": {
        "Tax amount": {
          "type": "sum"
        }
      }
    },
    "Cash summary.csv": {
      "column_mappings": {
        "Actual closeout cash": "Cash ($)",
        "Cash overage/shortage": "Over/Short ($)",
        "Actual deposit": "Cash ($)"
      }
    },
    "Sales by day.csv": {
      "column_mappings": {
        "Net sales": "Net Sales ($)",
        "Total orders": "Total Orders",
        "Total guests": "Lunch Covers"
      }
    },
    "Service Daypart summary.csv": {
      "column_mappings": {
        "Net sales": "Net Sales",
        "Orders": "Orders"
      },
      "rules": {
        "Net sales": {
          "type": "total"
        }
      }
    }
  }
}
